{
    "name": "awcodes/trov-installer",
    "description": "Trov CMS installer.",
    "keywords": [
        "laravel",
        "filament",
        "trov",
        "cms",
        "cli"
    ],
    "homepage": "https://github.com/TrovCMS/installer",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/TrovCMS/installer/issues",
        "source": "https://github.com/TrovCMS/installer"
    },
    "authors": [
        {
            "name": "Adam Weston",
            "email": "awcodes1@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "laravel-zero/framework": "^9.1.3",
        "nunomaduro/termwind": "^1.13",
        "spatie/fork": "^1.1",
        "spatie/laravel-ray": "^1.30"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "trov"
    ]
}